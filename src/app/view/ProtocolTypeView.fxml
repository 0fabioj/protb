<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="384.0" prefWidth="512.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.ProtocolTypeView">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0">
            <TitledPane animated="false" collapsible="false" text="Dados">
               <content>
                  <HBox prefHeight="0.0" prefWidth="494.0" styleClass="HBox">
                     <children>
                        <Label prefHeight="16.0" prefWidth="144.0" text="Descrição" />
                        <TextField fx:id="textfieldDesc" prefHeight="24.0" prefWidth="321.0" />
                        <TextField fx:id="textfieldId" editable="false" prefHeight="24.0" prefWidth="76.0" visible="false" />
                     </children>
                  </HBox>
               </content>
            </TitledPane>
            <TitledPane collapsible="false" prefHeight="258.0" prefWidth="512.0" text="Lista">
               <content>
                  <TableView fx:id="tableviewType" onMouseClicked="#actionEdit" prefHeight="246.0" prefWidth="494.0">
                    <columns>
                      <TableColumn fx:id="colDesc" prefWidth="314.0" text="Descrição" />
                    </columns>
                  </TableView>
               </content>
            </TitledPane>
      </VBox>
   </center>
   <VBox.margin>
      <Insets />
   </VBox.margin>
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#actionSave">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/save.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="buttonDel" mnemonicParsing="false" onAction="#actionDel">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/del.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#actionClear">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/erase.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
           <Button mnemonicParsing="false" onAction="#actionCancel">
              <graphic>
                 <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                       <Image url="@../../img/back.png" />
                    </image>
                 </ImageView>
              </graphic>
           </Button>
        </items>
      </ToolBar>
   </top>
   <bottom>
      <HBox styleClass="status-bar" BorderPane.alignment="CENTER">
         <children>
            <Label text="Label" />
         </children></HBox>
   </bottom>
</BorderPane>
